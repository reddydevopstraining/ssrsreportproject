  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="StageName">
          <DataField>StageName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NoofFactors">
          <DataField>NoofFactors</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="JobStatus">
          <DataField>JobStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT DISTINCT s.StageName, COUNT(qf.QualityFactor) AS 'NoofFactors', j.JobStatus
FROM   tbl_Stages AS s INNER JOIN
             tbl_QualityFactors AS qf ON s.StageId = qf.StageId INNER JOIN
             tbl_JobCardDetails AS j ON j.StageId = s.StageId
GROUP BY s.StageName, j.JobStatus</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
